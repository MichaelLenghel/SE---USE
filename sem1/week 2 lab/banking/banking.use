model BankingSys

class BankAccount
	attributes
		balance : Integer
		accountNo : String
	operations
		deposit(amount : Integer)
		begin
			self.balance := self.balance + amount;
		end
		
end	

class Customer
	attributes
		balance : Integer
		accountNo : String
	operations
		deposit(amount : Integer)
		begin
			self.balance := self.balance + amount;
		end
		
end	

